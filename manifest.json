{
   "manifest_version": 3,
   "name": "Jira, quality of life extension",
   "description": "Jira Datacenter (tested with Jira 9.5.1) is supported, Jira Cloud is not.",
   "version": "1.02",
   "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
   },
   "content_scripts": [
      {
         "js": [
            "js/dist.js"
         ],
         "matches": [
            "<all_urls>"
         ],
         "run_at": "document_idle"
      }
   ]
}